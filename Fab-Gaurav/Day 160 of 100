#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--)
    {
        int n;
        cin >> n;
        
        int a[n];
        int count = 0;
        int sum = 0;
        for(int i=1; i<=n; i++)
        {
            cin >> a[i];
        }
        
        for(int i=1; i<=n-1; i++)
        {
            for(int j=i+1; j<=n; j++)
            {
                if(a[i]>a[j])
                {
                    count++;
                }
            }
        }
        
        for(int i=1; i<=n-1; i++)
        {
            if(a[i]>a[i+1])
            {
                sum++;
            }
        }
        
        if(sum == count)
        {
            cout << "YES" << endl;
        }
        
        else
        {
            cout << "NO" << endl;
        }
    }

	return 0;
}
